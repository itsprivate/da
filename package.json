{
  "name": "example",
  "private": "true",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "archive": "npm run clean && trash .env && npm run build && trash ./static && cpy ./original-static ./static && cpy ./public ./static && echo GATSBY_ARCHIVE_TIME=$(npx date-cli) > .env",
    "archive:dev": "npm run clean && trash .env && npm run build:dev && trash ./static && cpy ./original-static ./static && cpy ./public ./static && echo GATSBY_ARCHIVE_TIME=$(npx date-cli) > .env",
    "start:prod": "LOCAL=false npm start",
    "build:dev": "LOCAL=true gatsby build",
    "upstream": "git remote add upstream git@github.com:itsprivate/gatsby-buzzing-starter.git"
  },
  "dependencies": {
    "@theowenyoung/gatsby-source-git": "^2.2.2",
    "cpy-cli": "^3.1.1",
    "date-cli": "^1.0.0",
    "gatsby": "^3.0.0",
    "gatsby-plugin-google-gtag": "^3.7.1",
    "gatsby-plugin-manifest": "^3.7.1",
    "gatsby-theme-buzzing": "^1.2.2",
    "i18next": "^19.4.5",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-i18next": "^11.11.0",
    "trash-cli": "^4.0.0"
  },
  "devDependencies": {
    "prettier": "^2.2.1"
  }
}
